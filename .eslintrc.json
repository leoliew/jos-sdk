{
  "extends": [
    "./node_modules/eslint-config-standard/eslintrc.json"
  ],
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true
  },
  "plugins": [
  ],
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "object-curly-spacing": [
      2,
      "never"
    ],
    "no-console": 1,
    "camelcase": 1
  }
}
